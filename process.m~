function[y] = process(x,h,N)
hsize = size(h);
L = hsize(2);
xsize = size(x)2)
H = fft(h,N);
M = N - L + 1;
P = floor(Lx/M);
for i = 0 : P - 1
   xb = x(i*M:(i+1)*M-1);
   yb = cc(xb,H);
   y(i*M:i*M+N-1) = y(i*M:i*M+N-1) + yb;
end
xb = x((P-1)*M:Lx-1);
yb = cc(xb,H);
y((P-1)*M:(P-1)*M+N-1) = y((P-1)*M:(P-1)*M+N-1) + yb;
end